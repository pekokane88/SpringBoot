{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#members}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/members}}
    </tbody>
</table>
<a href="/signup" class="btn btn-primary">Sign Up</a>
<a href="/members/{{members.id}}/edit" class="btn btn-primary">EDIT</a>
<a href="/members/{{members.id}}/delete" class="btn btn-danger">DELETE</a>
<a href="/members" class="btn btn-primary">Go to main</a>
{{>layouts/footer}}